<UserControl x:Class="Calculator.History"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Calculator"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="White">
        <StackPanel Margin="5">
            <Button HorizontalAlignment="Right" FontSize="12" Width="80" Click="Back_Click">Back</Button>
            <StackPanel Name="SPHistwory">
                <Label Content="My history"/>
                <Border BorderThickness="0.5" BorderBrush="Black" Margin="0,1"/>
                <StackPanel Name="myHis"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
